			MX8MN_IOMUXC_NAND_DATA02_QSPI_A_DATA2		0x84
			MX8MN_IOMUXC_NAND_DATA03_QSPI_A_DATA3		0x84
		>;
	};

	pinctrl_fpga: fpga_ctrlgrp {
		fsl,pins = <
			MX8MN_IOMUXC_SAI3_RXFS_GPIO4_IO28		0x5     /* prog_b */
			MX8MN_IOMUXC_SAI3_RXC_GPIO4_IO29		0x5     /* done */
			MX8MN_IOMUXC_SAI3_RXD_GPIO4_IO30     	0x5     /* init-b */
